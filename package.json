{
  "name": "leaflet-carousel",
  "author": "Adam Mertel",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/adammertel/leaflet-carousel/issues"
  },
  "homepage": "https://github.com/adammertel/leaflet-carousel#readme",
  "version": "0.1.7",
  "description": "leaflet experimental visualisation library based on leaflet-semi-circle",
  "main": "dist/leaflet-carousel.js",
  "files": [
    "dist",
    "src",
    "demo"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/adammertel/leaflet-carousel.git"
  },
  "keywords": [
    "leaflet",
    "visualisation"
  ],
  "dependencies": {
    "leaflet": "^1.0.3",
    "leaflet-semicircle": "^1.0.6"
  },
  "devDependencies": {
    "@comandeer/babel-plugin-banner": "^1.0.0",
    "babel-cli": "^6.24.1",
    "babel-plugin-transform-remove-console": "^6.8.3",
    "babel-preset-env": "^1.5.1",
    "babel-preset-es2015": "^6.24.1",
    "copyfiles": "^1.0.0",
    "jshint": "latest",
    "npm-run-all": "^4.0.2",
    "onchange": "^2.5.0"
  },
  "scripts": {
    "jshint": "jshint src",
    "babel": "babel src --out-file dist/leaflet-carousel.src.js --no-coments",
    "minify-js": "babel --plugins transform-remove-console dist/leaflet-carousel.src.js --out-file dist/leaflet-carousel.js --minified",
    "onchange-js": "onchange src/**/*.js -- npm run build",
    "onchange-css": "onchange src/**/*.css -- npm run move-css",
    "move-css": "copyfiles -f src/leaflet-carousel.css dist",
    "build": "run-s jshint babel minify-js move-css",
    "start": "run-p onchange-js build",
    "patch": "npm version patch",
    "patch-minor": "npm version minor",
    "publish": "npm publish",
    "update": "run-s patch publish",
    "update-minor": "run-s patch-minor publish"
  }
}
